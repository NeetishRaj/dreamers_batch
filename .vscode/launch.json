    "version": "0.2.0",
    "configurations": [
        {
            "name": "React Debugger",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}\\simple-routing"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Node Debug",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\debugging\\sample_code.js"
        }
    ]
}